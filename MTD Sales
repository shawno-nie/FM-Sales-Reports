   },
   "outputs": [],
   "source": [
    "###################################################################################\n",
    "# MAIN RTB BY MONTH:   SUB-TOTAL & TOTAL COLUMN  +  FORMATING                    #\n",
    "###################################################################################\n",
    "\n",
    "\n",
    "\"\"\"\n",
    "Creating new dataframes similar to the DBN, JHB etc dataframes we made for the graphs.\n",
    "We are adding a total row to the end of the regional dataframes, we create a new instance\n",
    "beacuse we do not want to corrupt the regional dataframes as they are used for the graphs\n",
    "\"\"\"\n",
    "def subtotal_report(rtb):\n",
    "    \n",
    "    DBN, JHB, PTA, CPT , reg_rtb = plot_data(rtb)\n",
    "       \n",
    "    DBN_t = rtb.loc[rtb[\"Region\"] ==\"Durban\"]\n",
    "    DBN_t = DBN_t.T\n",
    "    DBN_t[\"DBN TOTAL\"] = 0\n",
    "    DBN_t = DBN_t.T\n",
    "    DBN_t.groupby([\"WeekOfMonth\"],as_index=False).sum()\n",
    "\n",
    "    JHB_t = rtb.loc[rtb[\"Region\"] ==\"Johannesburg\"]\n",
    "    JHB_t = JHB_t.T\n",
    "    JHB_t[\"JHB TOTAL\"] = 0\n",
    "    JHB_t = JHB_t.T\n",
    "    JHB_t.groupby([\"WeekOfMonth\"],as_index=False).sum()\n",
    "\n",
    "    PTA_t = rtb.loc[rtb[\"Region\"] ==\"Pretoria\"]\n",
    "    PTA_t = PTA_t.T\n",
    "    PTA_t[\"PTA TOTAL\"] = 0\n",
    "    PTA_t = PTA_t.T\n",
    "    PTA_t.groupby([\"WeekOfMonth\"],as_index=False).sum()\n",
    "\n",
    "    CPT_t = rtb.loc[rtb[\"Region\"] ==\"Western Cape\"]\n",
    "    CPT_t = CPT_t.T\n",
    "    CPT_t[\"CPT TOTAL\"] = 0\n",
    "    CPT_t = CPT_t.T\n",
    "    CPT_t.groupby([\"WeekOfMonth\"],as_index=False).sum()\n",
    "\n",
    "\n",
    "    report = DBN_t\n",
    "    report = report.append(JHB_t)\n",
    "    report = report.append(PTA_t)\n",
    "    report = report.append(CPT_t)\n",
    "    report = report.T\n",
    "    report[\"FM TOTAL\"] = 0\n",
    "    report = report.T\n",
    "    rtb = report\n",
    "    \n",
    "        \n",
    "    \"\"\"\n",
    "    We now need to populate the data for the sub-total rows.( we will do it by region)\n",
    "    \"\"\"\n",
    "\n",
    "                                            #######################\n",
    "                                            # SUBTOTAL ROW DURBAN #\n",
    "                                            #######################\n",
    "    DBN_row_len = len(DBN[\"WeekOfMonth\"])\n",
    "    a = DBN_row_len  # this tells us how many weeks there are\n",
    "    rtb.iloc[DBN_row_len, 0] = \"\"\n",
    "    rtb.iloc[DBN_row_len, 1] = \"DBN TOTAL\"\n",
    "    # QTY\n",
    "    rtb.iloc[DBN_row_len, 2] = DBN[\"Qty\"].sum()\n",
    "    rtb.iloc[DBN_row_len, 3] = DBN[\"Qty_LY\"].sum()\n",
    "    grw = DBN[\"Qty\"].sum() - DBN[\"Qty_LY\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 4] = (grw / DBN[\"Qty_LY\"].sum()) * 100\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    rtb.iloc[DBN_row_len, 5] = DBN[\"Gross_Sales\"].sum()\n",
    "    rtb.iloc[DBN_row_len, 6] = DBN[\"GSales_LY\"].sum()\n",
    "    grw = DBN[\"Gross_Sales\"].sum() - DBN[\"GSales_LY\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 7] = (grw / DBN[\"GSales_LY\"].sum()) * 100\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 7] = \"NaN\"\n",
    "    # AUR\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 8] = rtb.iloc[DBN_row_len, 5] / rtb.iloc[DBN_row_len, 2]\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 9] = rtb.iloc[DBN_row_len, 6] / rtb.iloc[DBN_row_len, 3]\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 9] = \"NaN\"\n",
    "    rtb.iloc[DBN_row_len, 10] = rtb.iloc[DBN_row_len, 8] - rtb.iloc[DBN_row_len, 9]\n",
    "    # COST\n",
    "    rtb.iloc[DBN_row_len, 11] = DBN[\"Cost\"].sum()\n",
    "    rtb.iloc[DBN_row_len, 12] = DBN[\"Cost_LY\"].sum()\n",
    "    grw = DBN[\"Cost\"].sum() - DBN[\"Cost_LY\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 13] = (grw / DBN[\"Cost_LY\"].sum()) * 100\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 13] = \"NaN\"\n",
    "    # DISCOUNT\n",
    "    grw = DBN[\"Disc_TY\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 14] = grw / a\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 14] = \"NaN\"\n",
    "\n",
    "    grw = DBN[\"Disc_LY\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 15] = grw / a\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 15] = \"NaN\"\n",
    "    rtb.iloc[DBN_row_len, 16] = rtb.iloc[DBN_row_len, 14] - rtb.iloc[DBN_row_len, 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = DBN[\"GMargin\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[DBN_row_len, 17] = grw / a\n",
    "    except:\n",
    "        rtb.iloc[DBN_row_len, 17] = \"NaN\"\n",
    "\n",
    "\n",
    "                                            #############################\n",
    "                                            # SUBTOTAL ROW JOHANNESBURG #\n",
