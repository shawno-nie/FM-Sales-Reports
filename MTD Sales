    "    b = row_len  \n",
    "\n",
    "    rtb.iloc[row_len, 0] = \"\"\n",
    "    rtb.iloc[row_len, 1] = \"JHB TOTAL\"\n",
    "    # QTY\n",
    "    rtb.iloc[row_len, 2] = JHB[\"Qty\"].sum()\n",
    "    rtb.iloc[row_len, 3] = JHB[\"Qty_LY\"].sum()\n",
