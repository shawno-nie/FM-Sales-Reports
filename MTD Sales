    "                   name = f\"Sales Last Month\",\n",
    "                   mode = \"lines+markers\",\n",
    "                   line=dict(color='Orange', width=4),\n",
    "                   marker = dict(size = 16)) \n",
    "except: \n",
    "    print(\"Error\")\n",
    "    \n",
    "\n",
    "# 2) CREATE DATA\n",
    "op_pta_data = [op_pta_ty, op_pta_ly, op_pta_lm]\n",
    "\n",
    "# 3) CREATE LAYOUT\n",
