    "\n",
    "op_pta_layout = {\"title\":f\"PTA Sales:  {month} {ty} vs {ly}\",\n",
    "          \"xaxis\":{\"title\":\"Week of the Month\"},\n",
    "          \"yaxis\":{\"title\":f\"Optic Gross Sales for {month}\"}}\n",
    "\n",
    "# 4) DISPLAY GRAPH\n",
    "#iplot({\"data\":pta_data, \"layout\":pta_layout})\n",
    "                 #####################\n",
    "                 # REGION: CAPE TOWN #\n",
    "                 #####################\n",
    "\n",
    "# 1) CREATE TRACE\n",
    "try:\n",
    "    op_cpt_ty = go.Scatter(x = op_CPT[\"WeekOfMonth\"],\n",
    "                        y = op_CPT[\"Gross_Sales\"],\n",
    "                        name =  f\"CPT {ty}\",\n",
    "                        mode = \"lines+markers\",\n",
    "                        line=dict(color='blue', width=4),\n",
    "                        marker = dict(size = 16))\n",
    "    op_cpt_ly = go.Scatter(x = op_CPT[\"WeekOfMonth\"],\n",
    "                        y = op_CPT[\"GSales_LY\"],\n",
    "                        name =  f\"CPT {ly}\",\n",
    "                        mode = \"lines+markers\",\n",
    "                        line=dict(color='red', width=4),\n",
    "                        marker = dict(size = 16))\n",
    "    op_cpt_lm = go.Scatter(x = lm_CPT[\"WeekOfMonth\"],\n",
    "                   y = lm_CPT[\"GSales_LM\"],\n",
    "                   name = f\"Sales Last Month\",\n",
    "                   mode = \"lines+markers\",\n",
    "                   line=dict(color='Orange', width=4),\n",
    "                   marker = dict(size = 16)) \n",
    "except: \n",
    "    print(\"Error\")\n",
    "\n",
    "# 2) CREATE DATA\n",
    "op_cpt_data = [op_cpt_ty, op_cpt_ly, op_cpt_lm]\n",
    "\n",
    "# 3) CREATE LAYOUT\n",
    "\n",
    "\n",
    "op_cpt_layout = {\"title\":f\"CPT Sales:  {month} {ty} vs {ly}\",\n",
    "          \"xaxis\":{\"title\":\"Week of the Month\"},\n",
    "          \"yaxis\":{\"title\":f\"Optic Gross Sales for {month}\"} }\n",
    "\n",
    "\n",
    "# 4) DISPLAY GRAPH\n",
    "#iplot({\"data\":cpt_data, \"layout\":cpt_layout})\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 196,
   "metadata": {
    "extensions": {
     "jupyter_dashboards": {
      "version": 1,
      "views": {
       "grid_default": {},
       "report_default": {}
      }
     }
    }
   },
   "outputs": [],
   "source": [
    "###############\n",
    "# FINAL FORMAT \n",
    "\n",
    "rtb = pd.read_excel(f\"MTD_FM_1{month}.xlsx\", nrows=250, index_col=None)\n",
    "lm_rtb  = pd.read_excel(f\"MTD_FM_2{month}.xlsx\",nrows=250,index_col=None)\n",
    "rtb = rtb.replace([np.inf, -np.inf], 0) # Replace NaN and Inf\n",
    "rtb = rtb.replace([np.nan], 0) # Replace NaN and Inf\n",
    "lm_rtb = lm_rtb.replace([np.inf, -np.inf], 0) # Replace NaN and Inf\n",
    "lm_rtb = lm_rtb.replace([np.nan], 0) # Replace NaN and Inf\n",
    "\n",
    "op_rtb.to_excel(f\"MTD_Optic_1{month}.xlsx\", index=False)\n",
    "op_lm_rtb.to_excel(f\"MTD_Optic_2{month}.xlsx\", index=False)\n",
    "op_rtb = op_rtb.replace([np.inf, -np.inf], 0) # Replace NaN and Inf\n",
    "op_rtb = op_rtb.replace([np.nan], 0) # Replace NaN and Inf\n",
    "op_lm_rtb = op_lm_rtb.replace([np.inf, -np.inf], 0) # Replace NaN and Inf\n",
    "op_lm_rtb = op_lm_rtb.replace([np.nan], 0) # Replace NaN and Inf\n",
    "op_rtb = op_rtb.round()\n",
    "op_lm_rtb = op_lm_rtb.round()\n",
    "\n",
    "\n",
    "\n",
    "def color_negative_red(value):    \n",
    "    if value < 0:\n",
    "        color = 'red'\n",
    "    else:\n",
    "        color = 'black'\n",
    "    return 'color: %s' % color\n",
    "\n",
    " \n",
    "\n",
    "def custom_style(row):\n",
    "    sub = \"#B0C4DE\"\n",
    "    tot = '#7A67EE'\n",
    "    color = ''\n",
    "    \n",
    "    if row.loc[\"WeekOfMonth\"] == \"DBN TOTAL\":\n",
    "        color = sub\n",
    "    elif row.loc[\"WeekOfMonth\"] == \"JHB TOTAL\":\n",
    "        color = sub\n",
    "    elif row.loc[\"WeekOfMonth\"] == \"PTA TOTAL\":\n",
    "        color = sub\n",
    "    elif row.loc[\"WeekOfMonth\"] == \"CPT TOTAL\":\n",
    "        color = sub\n",
    "    elif row.loc[\"WeekOfMonth\"] == \"FM TOTAL\":\n",
    "        color = tot\n",
    "    return ['background-color: %s' % color]*len(row.values)\n",
    "\n",
    "def highlight_cols(s):\n",
    "    bcolor = \"#808080\"\n",
    "    return 'background-color: %s' % bcolor \n",
    "\n",
    "\n",
    "\n",
    "def style_dataframe(rtb):\n",
    "    rtb.columns.values[0]='Region'\n",
    "    rtb.loc[rtb[\"Region\"] == 0, \"Region\"] = \"\"\n",
    "    rtb = rtb.round(1)\n",
    "    rtb = (rtb.style.set_table_styles(\n",
    "           [{\n",
    "               'selector': 'th',\n",
    "               'props': [\n",
    "                   ('background-color', 'black'),  # Change the Header color\n",
    "                   ('color', 'white')]\n",
    "           }])\n",
    "            .applymap(color_negative_red, subset=['Qty', 'Qty_LY', 'Qty_Grw', 'Gross_Sales',\n",
    "           'GSales_LY', 'GSales_Grw', 'AUR', 'AUR_LY', 'AUR_Diff', 'Cost',\n",
    "           'Cost_LY', 'Cost_Grw', 'Disc_TY', 'Disc_LY', 'Disc_Diff', 'GMargin'])  # Highlight negative in red\n",
    "            .format({'Gross_Sales': \"R{:}\",\n",
    "                 'GSales_LY': \"R{:}\",\n",
    "                 'AUR': \"R{:}\",\n",
    "                 'AUR_LY': \"R{:}\",\n",
    "                 'AUR_Diff': \"R{:}\",\n",
    "                 'Cost': \"R{:}\",\n",
    "                 'Cost_LY': \"R{:}\",\n",
    "                 'GSales_Grw': \"{:.1%}\",\n",
    "                 'Qty': \"{:}\",\n",
    "                 'Qty_LY': \"{:}\",\n",
    "                 'Qty_Grw': \"{:.1%}\",\n",
    "                 'Cost_Grw': \"{:.1%}\",\n",
    "                 'Disc_TY': \"{:.1%}\",\n",
    "                 'Disc_LY': \"{:.1%}\",\n",
    "                 'Disc_Diff': \"{:.1%}\",\n",
    "                 'GMargin': \"{:.1%}\"})\n",
    "           .set_properties(**{'background-color' : '#F0F0F0',\n",
    "                              'border-color': 'black',\n",
    "                              'width' : None ,\n",
    "                              'text-align' : 'left',\n",
    "                              'border-width' : '1px',\n",
    "                              'font-size': '10pt',\n",
    "                              'border-style': 'solid'})\n",
    "           .applymap(highlight_cols, subset=pd.IndexSlice[:, ['WeekOfMonth', 'Region']])\n",
    "           .apply(custom_style, axis=1)\n",
    "            .hide_index())\n",
    "    return rtb\n",
    "\n",
    "\n",
    "def style_lm_dataframe(rtb):\n",
    "    rtb.columns.values[0]='Region'\n",
    "    rtb.loc[rtb[\"Region\"] == 0, \"Region\"] = \"\"\n",
    "    rtb = rtb.round(1)\n",
    "\n",
    "    rtb = (rtb.style.set_table_styles(\n",
    "           [{\n",
    "               'selector': 'th',\n",
    "               'props': [\n",
    "                   ('background-color', 'black'),  # Change the Header color\n",
    "                   ('color', 'white')]\n",
    "           }])\n",
    "            .applymap(color_negative_red, subset=['Qty', 'Qty_LM', 'Qty_Grw', 'Gross_Sales',\n",
    "           'GSales_LM', 'GSales_Grw', 'AUR', 'AUR_LM', 'AUR_Diff', 'Cost',\n",
    "           'Cost_LM', 'Cost_Grw', 'Disc_TM', 'Disc_LM', 'Disc_Diff', 'GMargin'])  # Highlight negative in red\n",
    "            .format({'Gross_Sales': \"R{:}\",\n",
    "                 'GSales_LM': \"R{:}\",\n",
    "                 'AUR': \"R{:}\",\n",
    "                 'AUR_LM': \"R{:}\",\n",
    "                 'AUR_Diff': \"R{:}\",\n",
    "                 'Cost': \"R{:}\",\n",
    "                 'Cost_LM': \"R{:}\",\n",
    "                 'GSales_Grw': \"{:.1%}\",\n",
    "                 'Qty': \"{:}\",\n",
    "                 'Qty_LM': \"{:}\",\n",
    "                 'Qty_Grw': \"{:.1%}\",\n",
    "                 'Cost_Grw': \"{:.1%}\",\n",
    "                 'Disc_TM': \"{:.1%}\",\n",
    "                 'Disc_LM': \"{:.1%}\",\n",
    "                 'Disc_Diff': \"{:.1%}\",\n",
    "                 'GMargin': \"{:.1%}\"})\n",
    "           .set_properties(**{'background-color' : '#F0F0F0',\n",
    "                              'border-color': 'black',\n",
    "                              'width' : None ,\n",
    "                              'text-align' : 'left',\n",
    "                              'border-width' : '1px',\n",
    "                              'font-size': '10pt',\n",
    "                              'border-style': 'solid'})\n",
    "           .applymap(highlight_cols, subset=pd.IndexSlice[:, ['WeekOfMonth', 'Region']])\n",
    "           .apply(custom_style, axis=1)\n",
    "            .hide_index())\n",
    "    return rtb\n"
   ]
  },
