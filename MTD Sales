  {
   "cell_type": "code",
   "execution_count": 187,
   "metadata": {
    "extensions": {
     "jupyter_dashboards": {
      "version": 1,
      "views": {
       "grid_default": {},
       "report_default": {}
      }
     }
    }
   },
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "###################################################################################\n",
    "#                           Setting up the DataFrame                              #\n",
    "###################################################################################\n",
    "\n",
    "dataframe = pd.read_csv(file_name)\n",
    "dataframe.columns.values[0]='delete' \n",
    "dataframe = dataframe.drop(columns=[\"delete\"], index=1)\n",
    "dataframe.reset_index(drop=False, inplace=True)\n",
    "pd.set_option('display.max_colwidth', 60)\n",
    "\n",
    "# FORMATTING \n",
    "\n",
    "\n",
    "\"\"\"Lets drop columns we are not using for RTB\"\"\"\n",
    "\n",
    "dataframe = dataframe.drop(columns=['Unique_ID','Weekday', \"RRP\",\n",
    "                  'ID_Num', 'Tax',  \n",
    "                  'ProductName', \n",
    "                  'Desc', \n",
    "                  'Profit', \n",
    "                  'Size',\n",
    "                  'Color', \n",
    "                  'Supplier', \n",
    "                  'Net_Sales'])\n",
    "dataframe[\"Regions\"] = dataframe[\"Region\"]\n",
    "\n",
    "\"\"\"\n",
    "NOw let's create new data frames for This Year and Last year same period. We will\n",
    "compate the data of each dataframe against the other to see growth \n",
    "\"\"\"\n",
    "\n",
    "def create_month_data(dataframe):\n",
    "\n",
    "                             ######################\n",
    "                             # Data for Last Year #\n",
    "                             ######################    \n",
    "    sly = dataframe.loc[(dataframe[\"Month\"] == month) & (dataframe[\"Year\"] == ly) ] # Filter Criteria\n",
    "    sly = sly.drop(columns=['Year',\"index\", \"DayOfMonth\", \"Inv #\"])                 # Don't want to see this\n",
    "    sly =sly.groupby([\"Month\", \"Region\", \"WeekOfMonth\"],as_index=False).sum()\n",
    "    sly[\"AUR\"] = (sly[\"Gross_Sales\"] / sly[\"Qty\"]).round()                          # Calc the AUR\n",
    "    sly[\"WeekOfMonth\"] = (sly.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x)) # add the word Week to week number \n",
    "\n",
    "                            ######################\n",
    "                            # Data for This Year #\n",
    "                            ######################\n",
    "    sty = dataframe.loc[(dataframe[\"Month\"] == month) & (dataframe[\"Year\"] == ty) ] # Filter Criteria\n",
    "    sty = sty.drop(columns=['Year',\"index\", \"DayOfMonth\", \"Inv #\"])                 # Don't want to see this\n",
    "    sty =sty.groupby([\"Month\", \"Region\", \"WeekOfMonth\"],as_index=False).sum()       # Pivot data\n",
    "    sty[\"AUR\"] = (sty[\"Gross_Sales\"] / sty[\"Qty\"]).round()                          # Calc the AUR\n",
    "    sty[\"WeekOfMonth\"] = (sty.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x)) # add the word Week to week number\n",
    "    \n",
    "    \n",
    "    \n",
    "                            ######################\n",
    "                            # Data for Last Month #\n",
    "                            ######################\n",
    "\n",
    "    slm = dataframe.loc[(dataframe[\"Month\"] == last_month) & (dataframe[\"Year\"] == ty)] # Filter Criteria\n",
    "    slm = slm.drop(columns=['Year', \"index\", \"DayOfMonth\", \"Inv #\"])                 # Don't want to see this\n",
    "    slm =slm.groupby([\"Month\", \"Region\", \"WeekOfMonth\"], as_index=False).sum()       # Pivot data\n",
    "    slm[\"AUR\"] = (slm[\"Gross_Sales\"] / slm[\"Qty\"]).round()                          # Calc the AUR\n",
    "    slm[\"WeekOfMonth\"] = (slm.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x)) # add the word Week to week number        \n",
    "\n",
    "                            ######################\n",
    "                            # Data for This Month #\n",
    "                            ######################\n",
    "\n",
    "    stm = dataframe.loc[(dataframe[\"Month\"] == month) & (dataframe[\"Year\"] == ty)] # Filter Criteria\n",
    "    stm = stm.drop(columns=['Year', \"index\", \"DayOfMonth\", \"Inv #\"])                 # Don't want to see this\n",
    "    stm =stm.groupby([\"Month\", \"Region\", \"WeekOfMonth\"], as_index=False).sum()       # Pivot data\n",
    "    stm[\"AUR\"] = (stm[\"Gross_Sales\"] / stm[\"Qty\"]).round()                          # Calc the AUR\n",
    "    stm[\"WeekOfMonth\"] = (stm.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x)) # add the word Week to week number\n",
    "    return sly, sty, slm, stm\n",
    "\n",
    "sly, sty, slm, stm = create_month_data(dataframe)\n",
    "\n",
    "def create_opticmonth_data(dataframe):\n",
    "    \n",
    "    op_sly = dataframe.loc[(dataframe[\"Month\"] == month) & (dataframe[\"Year\"] == ly)] # Fitler data\n",
    "\n",
    "    \"\"\" \n",
    "    Add all invoice numbers that has an optic lense in it to a list. We will use\n",
    "    we will use these numbers to identify all optic sales. Then also find all the\n",
    "    instances where there are Readers and add them to the dataframe\n",
    "    \"\"\"\n",
    "    temp_sly = op_sly.loc[(op_sly[\"Type\"]) ==\"optic_lenses\"]  \n",
    "    inv_list = []\n",
    "    for i in temp_sly[\"Inv #\"]:\n",
    "        if i not in inv_list:\n",
    "            inv_list.append(i)\n",
    "    op_sly = op_sly.loc[(op_sly[\"Inv #\"].isin(inv_list)) | (op_sly[\"Type\"] == \"Readers\")] # Extract all the data for the invoices in the list, as well as the data for Readers \n",
    "\n",
    "    op_sly = op_sly.drop(columns=['Year',\"index\", \"DayOfMonth\", \"Inv #\"])\n",
    "    op_sly =op_sly.groupby([\"Month\", \"Region\", \"WeekOfMonth\"],as_index=False).sum()\n",
    "    #Calculate AUR\n",
    "    op_sly[\"AUR\"] = (op_sly[\"Gross_Sales\"] / op_sly[\"Qty\"]).round()\n",
    "    op_sly[\"WeekOfMonth\"] = (op_sly.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x))\n",
    "    op_sly = op_sly.round()\n",
    "\n",
    "\n",
    "                                 ######################\n",
    "                                 # Data for This Year #\n",
    "                                 ######################\n",
    "\n",
    "\n",
    "    op_sty = dataframe.loc[(dataframe[\"Month\"] == month) & (dataframe[\"Year\"] == ty)] # Filter\n",
    "\n",
    "    temp_sty = op_sty.loc[(op_sty[\"Type\"]) == \"optic_lenses\"]\n",
    "    inv_list = []\n",
    "    for i in temp_sty[\"Inv #\"]:\n",
    "        if i not in inv_list:\n",
    "            inv_list.append(i)\n",
    "    op_sty = op_sty.loc[(op_sty[\"Inv #\"].isin(inv_list)) | (op_sty[\"Type\"] == \"Readers\")]\n",
    "\n",
    "    op_sty = op_sty.drop(columns=['Year', \"index\", \"DayOfMonth\", \"Inv #\"])\n",
    "    op_sty =op_sty.groupby([\"Month\", \"Region\", \"WeekOfMonth\"], as_index=False).sum()\n",
    "    op_sty[\"AUR\"] = (op_sty[\"Gross_Sales\"] / op_sty[\"Qty\"]).round()\n",
    "    op_sty[\"WeekOfMonth\"] = (op_sty.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x))\n",
    "    op_sty = op_sty.round()\n",
    "    \n",
    "\n",
    "    slm = dataframe.loc[(dataframe[\"Month\"] == last_month) & (dataframe[\"Year\"] == ty)] # Filter Criteria\n",
    "\n",
    "    temp_slm = slm.loc[(slm[\"Type\"]) ==\"optic_lenses\"]  \n",
    "    inv_list = []\n",
    "    for i in temp_slm[\"Inv #\"]:\n",
    "        if i not in inv_list:\n",
    "            inv_list.append(i)\n",
    "    slm = slm.loc[(slm[\"Inv #\"].isin(inv_list)) | (slm[\"Type\"] == \"Readers\")]\n",
    "\n",
    "    slm = slm.drop(columns=['Year', \"index\", \"DayOfMonth\", \"Inv #\"])                 # Don't want to see this\n",
    "    slm =slm.groupby([\"Month\", \"Region\", \"WeekOfMonth\"], as_index=False).sum()       # Pivot data\n",
    "    slm[\"AUR\"] = (slm[\"Gross_Sales\"] / slm[\"Qty\"]).round()                          # Calc the AUR\n",
    "    slm[\"WeekOfMonth\"] = (slm.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x)) # add the word Week to week number   \n",
    "    slm = slm.round()\n",
    "\n",
    "                                ######################\n",
    "                                # Data for This Month #\n",
    "                                ######################\n",
    "\n",
    "    stm = dataframe.loc[(dataframe[\"Month\"] == month) & (dataframe[\"Year\"] == ty)] # Filter Criteria\n",
    "\n",
    "    temp_stm = stm.loc[(stm[\"Type\"]) ==\"optic_lenses\"]  \n",
    "    inv_list = []\n",
    "    for i in temp_stm[\"Inv #\"]:\n",
    "        if i not in inv_list:\n",
    "            inv_list.append(i)\n",
    "    stm = stm.loc[(stm[\"Inv #\"].isin(inv_list)) | (stm[\"Type\"] == \"Readers\")]\n",
    "\n",
    "    stm = stm.drop(columns=['Year', \"index\", \"DayOfMonth\", \"Inv #\"])                 # Don't want to see this\n",
    "    stm =stm.groupby([\"Month\", \"Region\", \"WeekOfMonth\"], as_index=False).sum()       # Pivot data\n",
    "    stm[\"AUR\"] = (stm[\"Gross_Sales\"] / stm[\"Qty\"]).round()                          # Calc the AUR\n",
    "    stm[\"WeekOfMonth\"] = (stm.WeekOfMonth.astype(str).apply(lambda x: \"Week \" + x)) # add the word Week to week number  \n",
    "    stm = stm.round()\n",
    "    op_slm = slm\n",
    "    op_stm = stm\n",
    "    return op_sly, op_sty, op_slm, op_stm\n",
    "\n",
    "op_sly, op_sty, op_slm, op_stm = create_opticmonth_data(dataframe)\n",
    "\n",
    "\n",
    "###################################################################################\n",
    "# MAIN RTB BY MONTH:           CREATION                                           #\n",
    "###################################################################################\n",
    "# Merge TY and LY datasets\n",
    "rtb = pd.merge(sty, sly, on=[\"Region\", \"WeekOfMonth\"], how = \"outer\").fillna(0)\n",
    "lm_rtb = pd.merge(stm, slm, on=[\"Region\", \"WeekOfMonth\"], how = \"outer\").fillna(0)\n",
    "op_rtb = pd.merge(op_sty, op_sly, on=[\"Region\", \"WeekOfMonth\"], how = \"outer\").fillna(0)\n",
    "op_lm_rtb = pd.merge(op_stm, op_slm, on=[\"Region\", \"WeekOfMonth\"], how = \"outer\").fillna(0)\n",
    "\n",
    "###################################################################################\n",
    "# MAIN RTB BY MONTH:           FORMAT VIEW, RENAME COLUMNS & CALCULATE            #\n",
    "###################################################################################\n",
    "\n",
    "\n",
    "def merged_change_layout(rtb):\n",
    "    \n",
    "    new = rtb.rename(columns={'Qty_x': 'Qty',\n",
    "                             \"AUR_x\": 'AUR',\n",
    "                             'Cat': 'Category',\n",
    "                             \"AUR_y\": 'AUR_LY',\n",
    "                             'Gross_Sales_x':'Gross_Sales', \n",
    "                             'Cost_x':\"Cost\", \n",
    "                             'Discount_x':\"TY_Disc\", \n",
    "                             'Qty_y':\"Qty_LY\",\n",
    "                             'Gross_Sales_y':\"GSales_LY\", \n",
    "                             'Cost_y':\"Cost_LY\", \n",
    "                             'Discount_y':\"LY_Disc\"})\n",
    "\n",
    "                        # GROSS MARGIN & AUR #\n",
    "    new[\"AUR\"] = (new[\"Gross_Sales\"] / new[\"Qty\"]).round()\n",
    "    calc =  new[\"Gross_Sales\"] - new[\"Cost\"]\n",
    "    new[\"GMargin\"] = ((calc / new[\"Gross_Sales\"])*100).round()\n",
    "\n",
    "                        # GROWTH ON LY #\n",
    "\n",
    "    # QTY\n",
    "    calc = new[\"Qty\"] - new[\"Qty_LY\"]\n",
    "    new[\"Qty_Grw\"] = ((calc / new[\"Qty_LY\"])*100).round()   \n",
    "\n",
    "    # GROSS SALES\n",
    "    calc = new[\"Gross_Sales\"] -new[\"GSales_LY\"]\n",
    "    new[\"GSales_Grw\"] = ((calc / new[\"GSales_LY\"])*100).round()   \n",
    "\n",
    "    # AUR DIFFERENCE\n",
    "    new[\"AUR_Diff\"] =  (new[\"AUR\"] - new[\"AUR_LY\"] ).round()\n",
    "\n",
    "    # COST\n",
    "    calc = new[\"Cost\"] - new[\"Cost_LY\"]\n",
    "    new[\"Cost_Grw\"] = ((calc / new[\"Cost_LY\"])*100).round()   \n",
    "\n",
    "    # DISCOUNT\n",
    "    calc = new[\"Gross_Sales\"] + new[\"TY_Disc\"]\n",
    "    new[\"Disc_TY\"] = ((new[\"TY_Disc\"] / calc)*100).round()\n",
    "\n",
    "    calc = new[\"LY_Disc\"] + new[\"GSales_LY\"]\n",
    "    new[\"Disc_LY\"] = ((new[\"LY_Disc\"] / calc)*100).round()\n",
    "\n",
    "    new[\"Disc_Diff\"] = new[\"Disc_TY\"] - new[\"Disc_LY\"]\n",
    "\n",
    "    # FORMAT LAYOUT\n",
    "    new2 = new[['Region', 'WeekOfMonth','Qty', 'Qty_LY', \"Qty_Grw\", \n",
    "               'Gross_Sales','GSales_LY', \"GSales_Grw\",\n",
    "               'AUR', 'AUR_LY', \"AUR_Diff\",\n",
    "               'Cost', 'Cost_LY', \"Cost_Grw\",\n",
    "               \"Disc_TY\", \"Disc_LY\", \"Disc_Diff\",\n",
    "               'GMargin']]\n",
    "    return new2\n",
    "\n",
    "def merged_change_lm(lm_rtb):    \n",
    "    lm_rtb= lm_rtb.rename(columns={'Qty_x': 'Qty',\n",
    "                             \"AUR_x\": 'AUR',\n",
    "                             'Cat': 'Category',\n",
    "                             \"AUR_y\": 'AUR_LM',\n",
    "                             'Gross_Sales_x':'Gross_Sales',\n",
    "                             'Cost_x':\"Cost\",\n",
    "                             'Discount_x':\"TM_Disc\",\n",
    "                             'Qty_y':\"Qty_LM\",\n",
    "                             'Gross_Sales_y':\"GSales_LM\",\n",
    "                             'Cost_y':\"Cost_LM\",\n",
    "                             'Discount_y':\"LM_Disc\"})\n",
    "\n",
    "    lm_rtb[\"AUR\"] = (lm_rtb[\"Gross_Sales\"] / lm_rtb[\"Qty\"]).round()\n",
    "    calc = lm_rtb[\"Gross_Sales\"] - lm_rtb[\"Cost\"]\n",
    "    lm_rtb[\"GMargin\"] = ((calc / lm_rtb[\"Gross_Sales\"]) * 100).round()\n",
    "\n",
    " \n",
    "                            # GROWTH ON LM #\n",
    "     # QTY\n",
    "    calc = lm_rtb[\"Qty\"] - lm_rtb[\"Qty_LM\"]\n",
    "    lm_rtb[\"Qty_Grw\"] = ((calc / lm_rtb[\"Qty_LM\"]) * 100).round()\n",
    "\n",
    "    # GROSS SALES\n",
    "    calc = lm_rtb[\"Gross_Sales\"] - lm_rtb[\"GSales_LM\"]\n",
    "    lm_rtb[\"GSales_Grw\"] = ((calc / lm_rtb[\"GSales_LM\"]) * 100).round()\n",
    "\n",
    "    # AUR DIFFERENCE\n",
    "    lm_rtb[\"AUR_Diff\"] =  (lm_rtb[\"AUR\"] - lm_rtb[\"AUR_LM\"]).round()\n",
    "\n",
    "    # COST\n",
    "    calc = lm_rtb[\"Cost\"] - lm_rtb[\"Cost_LM\"]\n",
    "    lm_rtb[\"Cost_Grw\"] = ((calc / lm_rtb[\"Cost_LM\"]) * 100).round()\n",
    "\n",
    "    # DISCOUNT\n",
    "    calc = lm_rtb[\"Gross_Sales\"] + lm_rtb[\"TM_Disc\"]\n",
    "    lm_rtb[\"Disc_TM\"] = ((lm_rtb[\"TM_Disc\"] / calc) * 100).round()\n",
    "\n",
    "    calc = lm_rtb[\"LM_Disc\"] + lm_rtb[\"GSales_LM\"]\n",
    "    lm_rtb[\"Disc_LM\"] = ((lm_rtb[\"LM_Disc\"] / calc) * 100).round()\n",
    "\n",
    "    lm_rtb[\"Disc_Diff\"] = lm_rtb[\"Disc_TM\"] - lm_rtb[\"Disc_LM\"]\n",
    "\n",
    "    # FORMAT LAYOUT\n",
    "    lm_rtb = lm_rtb[['Region', 'WeekOfMonth', 'Qty', 'Qty_LM', \"Qty_Grw\",\n",
    "               'Gross_Sales','GSales_LM', \"GSales_Grw\",\n",
    "               'AUR', 'AUR_LM', \"AUR_Diff\",\n",
    "               'Cost', 'Cost_LM', \"Cost_Grw\",\n",
    "               \"Disc_TM\", \"Disc_LM\", \"Disc_Diff\",\n",
    "               'GMargin']]\n",
    "    return lm_rtb\n",
    "\n",
    "op_rtb = merged_change_layout(op_rtb)\n",
    "op_lm_rtb = merged_change_lm(op_lm_rtb)\n",
    "rtb = merged_change_layout(rtb)\n",
    "lm_rtb = merged_change_lm(lm_rtb)\n",
    "\n",
    "def plot_data(rtb):\n",
    "    DBN = rtb.loc[rtb[\"Region\"] ==\"Durban\"]\n",
    "    JHB = rtb.loc[rtb[\"Region\"] ==\"Johannesburg\"]\n",
    "    PTA = rtb.loc[rtb[\"Region\"] ==\"Pretoria\"]\n",
    "    CPT = rtb.loc[rtb[\"Region\"] ==\"Western Cape\"]\n",
    "    report = DBN\n",
    "    report = report.append(JHB)\n",
    "    report = report.append(PTA)\n",
    "    report = report.append(CPT)\n",
    "    reg_rtb = report\n",
    "    return DBN, JHB, PTA, CPT , reg_rtb\n",
    "\n",
    "def lm_plot_data(lm_rtb):\n",
    "    lm_DBN = lm_rtb.loc[lm_rtb[\"Region\"] ==\"Durban\"]\n",
    "    lm_JHB = lm_rtb.loc[lm_rtb[\"Region\"] ==\"Johannesburg\"]\n",
    "    lm_PTA = lm_rtb.loc[lm_rtb[\"Region\"] ==\"Pretoria\"]\n",
    "    lm_CPT = lm_rtb.loc[lm_rtb[\"Region\"] ==\"Western Cape\"]\n",
    "    \n",
    "    lm_reg_rtb = lm_DBN\n",
    "    lm_reg_rtb = lm_reg_rtb.append(lm_JHB)\n",
    "    lm_reg_rtb = lm_reg_rtb.append(lm_PTA)\n",
    "    lm_reg_rtb = lm_reg_rtb.append(lm_CPT)\n",
    "    return lm_DBN, lm_JHB, lm_PTA, lm_CPT , lm_reg_rtb\n",
    "lm_DBN, lm_JHB, lm_PTA, lm_CPT , lm_reg_rtb = lm_plot_data(lm_rtb)"
   ]
  },
