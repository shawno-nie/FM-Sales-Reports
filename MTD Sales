    "    except:\n",
    "        rtb.iloc[row_len, 15] = \"NaN\"\n",
    "    rtb.iloc[row_len, 16] = rtb.iloc[row_len, 14] - rtb.iloc[row_len, 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = CPT[\"GMargin\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[row_len, 17] = grw / a\n",
    "    except:\n",
    "        rtb.iloc[row_len, 17] = \"NaN\"\n",
    "\n",
    "                                            ###################\n",
    "                                            # GRAND TOTAL ROW #\n",
    "                                            ###################\n",
    "    row_len = row_len + 1\n",
    "    rtb.iloc[row_len,0] = \"\"\n",
    "    rtb.iloc[row_len,1] = \"FM TOTAL\"\n",
    "    #QTY\n",
    "    rtb.iloc[row_len,2] = (rtb.iloc[a,2]+rtb.iloc[b,2]+rtb.iloc[c,2]+rtb.iloc[d,2]).round()\n",
    "    rtb.iloc[row_len,3] = (rtb.iloc[a,3]+rtb.iloc[b,3]+rtb.iloc[c,3]+rtb.iloc[d,3]).round()\n",
    "    grw = rtb.iloc[row_len,2] - rtb.iloc[row_len,3]\n",
    "    try:\n",
    "        rtb.iloc[row_len,4] = (grw / rtb.iloc[row_len,3]) * 100\n",
    "    except:\n",
    "        rtb.iloc[row_len,4] = \"NaN\"\n",
    "    #GROSS SALES\n",
    "    rtb.iloc[row_len,5] = (rtb.iloc[a,5]+rtb.iloc[b,5]+rtb.iloc[c,5]+rtb.iloc[d,5]).round()\n",
    "    rtb.iloc[row_len,6] = (rtb.iloc[a,6]+rtb.iloc[b,6]+rtb.iloc[c,6]+rtb.iloc[d,6]).round()\n",
    "    grw = rtb.iloc[row_len,5] - rtb.iloc[row_len,6]\n",
    "    try:\n",
    "        rtb.iloc[row_len,7] = (grw / rtb.iloc[row_len,6]) * 100\n",
    "    except:\n",
    "        rtb.iloc[row_len,7] = \"NaN\"\n",
    "    #AUR\n",
    "    try:\n",
    "        rtb.iloc[row_len,8] = (rtb.iloc[row_len,5] / rtb.iloc[row_len,2]).round()\n",
    "    except:\n",
    "        rtb.iloc[row_len,8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        rtb.iloc[row_len,9] = (rtb.iloc[row_len,6] / rtb.iloc[row_len,3]).round()\n",
    "    except:\n",
    "        rtb.iloc[row_len,9] =  \"NaN\"\n",
    "    rtb.iloc[row_len,10] = (rtb.iloc[row_len,8] - rtb.iloc[row_len,9]).round()\n",
    "    #COST\n",
    "    rtb.iloc[row_len,11] = (rtb.iloc[a,11]+rtb.iloc[b,11]+rtb.iloc[c,11]+rtb.iloc[d,11]).round()\n",
    "    rtb.iloc[row_len,12] = (rtb.iloc[a,12]+rtb.iloc[b,12]+rtb.iloc[c,12]+rtb.iloc[d,12]).round()\n",
    "    grw = rtb.iloc[row_len,11] - rtb.iloc[row_len,12]\n",
    "    try:\n",
    "        rtb.iloc[row_len,13] = (grw / rtb.iloc[row_len,12]) * 100\n",
    "    except:\n",
    "        rtb.iloc[row_len,13] = \"NaN\"\n",
    "    #DISCOUNT\n",
    "    grw = rtb.iloc[a,14]+rtb.iloc[b,14]+rtb.iloc[c,14]+rtb.iloc[d,14]\n",
    "    try:\n",
    "        rtb.iloc[row_len,14] = (grw/4).round()\n",
    "    except:\n",
    "        rtb.iloc[row_len,14] = \"NaN\"\n",
    "\n",
    "    grw = rtb.iloc[a,15]+rtb.iloc[b,15]+rtb.iloc[c,15]+rtb.iloc[d,15]\n",
    "    try:\n",
    "        rtb.iloc[row_len,15] = (grw/4).round()\n",
    "    except: \n",
    "        rtb.iloc[row_len,15] = \"NaN\"\n",
    "    rtb.iloc[row_len,16] = rtb.iloc[row_len,14] - rtb.iloc[row_len,15]\n",
    "    #GROSS MARGIN\n",
    "    grw = rtb.iloc[a,17]+rtb.iloc[b,17]+rtb.iloc[c,17]+rtb.iloc[d,17]\n",
    "    try:\n",
    "        rtb.iloc[row_len,17] = (grw/4).round()\n",
    "    except:\n",
    "        rtb.iloc[row_len,17] = \"NaN\"\n",
    "\n",
    "    rtb[\"Qty_Grw\"] = rtb[\"Qty_Grw\"] / 100\n",
    "    rtb[\"GSales_Grw\"] = rtb[\"GSales_Grw\"] / 100\n",
    "    rtb[\"Cost_Grw\"] = rtb[\"Cost_Grw\"] / 100\n",
    "    rtb[\"Disc_TY\"] = rtb[\"Disc_TY\"] / 100\n",
    "    rtb[\"Disc_LY\"] = rtb[\"Disc_LY\"] / 100\n",
    "    rtb[\"Disc_Diff\"] = rtb[\"Disc_Diff\"] / 100\n",
    "    rtb[\"GMargin\"] = rtb[\"GMargin\"] / 100\n",
    "    rtb = rtb.replace([np.inf, -np.inf], 0) # Replace NaN and Inf\n",
    "    rtb = rtb.replace([np.nan], 0) # Replace NaN and Inf\n",
    "\n",
    "    return rtb\n",
    "\n",
    "def lm_subtotal_report(lm_rtb):\n",
    "    lw_DBN, lw_JHB, lw_PTA, lw_CPT , lw_reg_rtb = lm_plot_data(lm_rtb)\n",
    "    DBN_lm = lm_rtb.loc[lm_rtb[\"Region\"] == \"Durban\"]\n",
    "    DBN_lm = DBN_lm.T\n",
    "    DBN_lm[\"DBN TOTAL\"] = 0\n",
    "    DBN_lm = DBN_lm.T\n",
    "    DBN_lm.groupby([\"WeekOfMonth\"], as_index=False).sum()\n",
    "\n",
    "    JHB_lm = lm_rtb.loc[lm_rtb[\"Region\"] == \"Johannesburg\"]\n",
    "    JHB_lm = JHB_lm.T\n",
    "    JHB_lm[\"JHB TOTAL\"] = 0\n",
    "    JHB_lm = JHB_lm.T\n",
    "    JHB_lm.groupby([\"WeekOfMonth\"], as_index=False).sum()\n",
    "\n",
    "    PTA_lm = lm_rtb.loc[lm_rtb[\"Region\"] == \"Pretoria\"]\n",
    "    PTA_lm = PTA_lm.T\n",
    "    PTA_lm[\"PTA TOTAL\"] = 0\n",
    "    PTA_lm = PTA_lm.T\n",
    "    PTA_lm.groupby([\"WeekOfMonth\"], as_index=False).sum()\n",
    "\n",
    "    CPT_lm = lm_rtb.loc[lm_rtb[\"Region\"] == \"Western Cape\"]\n",
    "    CPT_lm = CPT_lm.T\n",
    "    CPT_lm[\"CPT TOTAL\"] = 0\n",
    "    CPT_lm = CPT_lm.T\n",
    "    CPT_lm.groupby([\"WeekOfMonth\"], as_index=False).sum()\n",
    "\n",
    "\n",
    "    report_lm = DBN_lm\n",
    "    report_lm = report_lm.append(JHB_lm)\n",
    "    report_lm = report_lm.append(PTA_lm)\n",
    "    report_lm = report_lm.append(CPT_lm)\n",
    "    report_lm = report_lm.T\n",
    "    report_lm[\"FM TOTAL\"] = 0\n",
    "    report_lm = report_lm.T\n",
    "    lm_rtb = report_lm\n",
    "\n",
    "\n",
    "\n",
    "    \"\"\"\n",
    "    We now need to populate the data for the sub-total rows.( we will do it by region)\n",
    "    \"\"\"\n",
    "\n",
    "    #######################\n",
    "    # SUBTOTAL ROW DURBAN #\n",
    "    #######################\n",
    "    DBN_row_len = len(lw_DBN[\"WeekOfMonth\"])\n",
    "    a = DBN_row_len  # this tells us how many weeks there are\n",
    "    lm_rtb.iloc[DBN_row_len, 0] = \"\"\n",
    "    lm_rtb.iloc[DBN_row_len, 1] = \"DBN TOTAL\"\n",
    "    # QTY\n",
    "    lm_rtb.iloc[DBN_row_len, 2] = lw_DBN[\"Qty\"].sum()\n",
    "    lm_rtb.iloc[DBN_row_len, 3] = lw_DBN[\"Qty_LM\"].sum()\n",
    "    grw = lw_DBN[\"Qty\"].sum() - lw_DBN[\"Qty_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 4] = (grw / lw_DBN[\"Qty_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    lm_rtb.iloc[DBN_row_len, 5] = lw_DBN[\"Gross_Sales\"].sum()\n",
    "    lm_rtb.iloc[DBN_row_len, 6] = lw_DBN[\"GSales_LM\"].sum()\n",
    "    grw = lw_DBN[\"Gross_Sales\"].sum() - lw_DBN[\"GSales_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 7] = (grw / lw_DBN[\"GSales_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 7] = \"NaN\"\n",
    "    # AUR\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 8] = lm_rtb.iloc[DBN_row_len, 5] / lm_rtb.iloc[DBN_row_len, 2]\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 9] = lm_rtb.iloc[DBN_row_len, 6] / lm_rtb.iloc[DBN_row_len, 3]\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 9] = \"NaN\"\n",
    "    lm_rtb.iloc[DBN_row_len, 10] = lm_rtb.iloc[DBN_row_len, 8] - lm_rtb.iloc[DBN_row_len, 9]\n",
    "    # COST\n",
    "    lm_rtb.iloc[DBN_row_len, 11] = lw_DBN[\"Cost\"].sum()\n",
    "    lm_rtb.iloc[DBN_row_len, 12] = lw_DBN[\"Cost_LM\"].sum()\n",
    "    grw = lw_DBN[\"Cost\"].sum() - lw_DBN[\"Cost_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 13] = (grw / lw_DBN[\"Cost_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 13] = \"NaN\"\n",
    "    # DISCOUNT\n",
    "    grw = lw_DBN[\"Disc_TM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 14] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 14] = \"NaN\"\n",
    "\n",
    "    grw = lw_DBN[\"Disc_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 15] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 15] = \"NaN\"\n",
    "    lm_rtb.iloc[DBN_row_len, 16] = lm_rtb.iloc[DBN_row_len, 14] - lm_rtb.iloc[DBN_row_len, 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = lw_DBN[\"GMargin\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[DBN_row_len, 17] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[DBN_row_len, 17] = \"NaN\"\n",
    "\n",
    "\n",
    "        #############################\n",
    "        # SUBTOTAL ROW JOHANNESBURG #\n",
    "        #############################\n",
    "\n",
    "    JHB_row_len = len(lw_JHB[\"WeekOfMonth\"])\n",
    "    row_len = (DBN_row_len + 1) + JHB_row_len\n",
    "    b = row_len\n",
    "\n",
    "    lm_rtb.iloc[row_len, 0] = \"\"\n",
    "    lm_rtb.iloc[row_len, 1] = \"JHB TOTAL\"\n",
    "    # QTY\n",
    "    lm_rtb.iloc[row_len, 2] = lw_JHB[\"Qty\"].sum()\n",
    "    lm_rtb.iloc[row_len, 3] = lw_JHB[\"Qty_LM\"].sum()\n",
    "    grw = lw_JHB[\"Qty\"].sum() - lw_JHB[\"Qty_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 4] = (grw / lw_JHB[\"Qty_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    lm_rtb.iloc[row_len, 5] = lw_JHB[\"Gross_Sales\"].sum()\n",
    "    lm_rtb.iloc[row_len, 6] = lw_JHB[\"GSales_LM\"].sum()\n",
    "    grw = lw_JHB[\"Gross_Sales\"].sum() - lw_JHB[\"GSales_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 7] = (grw / lw_JHB[\"GSales_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 7] = \"NaN\"\n",
    "    # AUR\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 8] = lm_rtb.iloc[row_len, 5] / lm_rtb.iloc[row_len, 2]\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 9] = lm_rtb.iloc[row_len, 6] / lm_rtb.iloc[row_len, 3]\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 9] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len, 10] = lm_rtb.iloc[row_len, 8] - lm_rtb.iloc[row_len, 9]\n",
    "    # COST\n",
    "    lm_rtb.iloc[row_len, 11] = lw_JHB[\"Cost\"].sum()\n",
    "    lm_rtb.iloc[row_len, 12] = lw_JHB[\"Cost_LM\"].sum()\n",
    "    grw = lw_JHB[\"Cost\"].sum() - lw_JHB[\"Cost_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 13] = (grw / lw_JHB[\"Cost_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 13] = \"NaN\"\n",
    "    # DISCOUNT\n",
    "    grw = lw_JHB[\"Disc_TM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 14] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 14] = \"NaN\"\n",
    "\n",
    "    grw = lw_JHB[\"Disc_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 15] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 15] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len, 16] = lm_rtb.iloc[row_len, 14] - lm_rtb.iloc[row_len, 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = lw_JHB[\"GMargin\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 17] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 17] = \"NaN\"\n",
    "\n",
    "        #########################\n",
    "        # SUBTOTAL ROW PRETORIA #\n",
    "        #########################\n",
    "\n",
    "    PTA_row_len = len(lw_PTA[\"WeekOfMonth\"])\n",
    "    row_len = (row_len + 1) + PTA_row_len\n",
    "    c = row_len\n",
    "\n",
    "    lm_rtb.iloc[row_len, 0] = \"\"\n",
    "    lm_rtb.iloc[row_len, 1] = \"PTA TOTAL\"\n",
    "    # QTY\n",
    "    lm_rtb.iloc[row_len, 2] = lw_PTA[\"Qty\"].sum()\n",
    "    lm_rtb.iloc[row_len, 3] = lw_PTA[\"Qty_LM\"].sum()\n",
    "    grw = lw_PTA[\"Qty\"].sum() - lw_PTA[\"Qty_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 4] = (grw / lw_PTA[\"Qty_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    lm_rtb.iloc[row_len, 5] = lw_PTA[\"Gross_Sales\"].sum()\n",
    "    lm_rtb.iloc[row_len, 6] = lw_PTA[\"GSales_LM\"].sum()\n",
    "    grw = lw_PTA[\"Gross_Sales\"].sum() - lw_PTA[\"GSales_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 7] = (grw / lw_PTA[\"GSales_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 7] = \"NaN\"\n",
    "    # AUR\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 8] = lm_rtb.iloc[row_len, 5] / lm_rtb.iloc[row_len, 2]\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 9] = lm_rtb.iloc[row_len, 6] / lm_rtb.iloc[row_len, 3]\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 9] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len, 10] = lm_rtb.iloc[row_len, 8] - lm_rtb.iloc[row_len, 9]\n",
    "    # COST\n",
    "    lm_rtb.iloc[row_len, 11] = lw_PTA[\"Cost\"].sum()\n",
    "    lm_rtb.iloc[row_len, 12] = lw_PTA[\"Cost_LM\"].sum()\n",
    "    grw = lw_PTA[\"Cost\"].sum() - lw_PTA[\"Cost_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 13] = (grw / lw_PTA[\"Cost_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 13] = \"NaN\"\n",
    "    # DISCOUNT\n",
    "    grw = lw_PTA[\"Disc_TM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 14] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 14] = \"NaN\"\n",
    "\n",
    "    grw = lw_PTA[\"Disc_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 15] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 15] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len, 16] = lm_rtb.iloc[row_len, 14] - lm_rtb.iloc[row_len, 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = lw_PTA[\"GMargin\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 17] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 17] = \"NaN\"\n",
    "\n",
    "        ##########################\n",
    "        # SUBTOTAL ROW CAPE TOWN #\n",
    "        ##########################\n",
    "\n",
    "    CPT_row_len = len(lw_CPT[\"WeekOfMonth\"])\n",
    "    row_len = (row_len + 1) + CPT_row_len\n",
    "    d = row_len\n",
    "\n",
    "    lm_rtb.iloc[row_len, 0] = \"\"\n",
    "    lm_rtb.iloc[row_len, 1] = \"CPT TOTAL\"\n",
    "    # QTY\n",
    "    lm_rtb.iloc[row_len, 2] = lw_CPT[\"Qty\"].sum()\n",
    "    lm_rtb.iloc[row_len, 3] = lw_CPT[\"Qty_LM\"].sum()\n",
    "    grw = lw_CPT[\"Qty\"].sum() - lw_CPT[\"Qty_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 4] = (grw / lw_CPT[\"Qty_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    lm_rtb.iloc[row_len, 5] = lw_CPT[\"Gross_Sales\"].sum()\n",
    "    lm_rtb.iloc[row_len, 6] = lw_CPT[\"GSales_LM\"].sum()\n",
    "    grw = lw_CPT[\"Gross_Sales\"].sum() - lw_CPT[\"GSales_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 7] = (grw / lw_CPT[\"GSales_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 7] = \"NaN\"\n",
    "    # AUR\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 8] = lm_rtb.iloc[row_len, 5] / lm_rtb.iloc[row_len, 2]\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 9] = lm_rtb.iloc[row_len, 6] / lm_rtb.iloc[row_len, 3]\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 9] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len, 10] = lm_rtb.iloc[row_len, 8] - lm_rtb.iloc[row_len, 9]\n",
    "    # COST\n",
    "    lm_rtb.iloc[row_len, 11] = lw_CPT[\"Cost\"].sum()\n",
    "    lm_rtb.iloc[row_len, 12] = lw_CPT[\"Cost_LM\"].sum()\n",
    "    grw = lw_CPT[\"Cost\"].sum() - lw_CPT[\"Cost_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 13] = (grw / lw_CPT[\"Cost_LM\"].sum()) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 13] = \"NaN\"\n",
    "    # DISCOUNT\n",
    "    grw = lw_CPT[\"Disc_TM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 14] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 14] = \"NaN\"\n",
    "\n",
    "    grw = lw_CPT[\"Disc_LM\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 15] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 15] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len, 16] = lm_rtb.iloc[row_len, 14] - lm_rtb.iloc[row_len, 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = lw_CPT[\"GMargin\"].sum()\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len, 17] = grw / a\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len, 17] = \"NaN\"\n",
    "\n",
    "        ###################\n",
    "        # GRAND TOTAL ROW #\n",
    "        ###################\n",
    "    row_len = row_len + 1\n",
    "    lm_rtb.iloc[row_len , 0] = \"\"\n",
    "    lm_rtb.iloc[row_len , 1] = \"FM TOTAL\"\n",
    "    # QTY\n",
    "    lm_rtb.iloc[row_len , 2] = (lm_rtb.iloc[a , 2] + lm_rtb.iloc[b , 2] + lm_rtb.iloc[c , 2] + lm_rtb.iloc[d , 2]).round()\n",
    "    lm_rtb.iloc[row_len , 3] = (lm_rtb.iloc[a , 3] + lm_rtb.iloc[b , 3] + lm_rtb.iloc[c , 3] + lm_rtb.iloc[d , 3]).round()\n",
    "    grw = lm_rtb.iloc[row_len , 2] - lm_rtb.iloc[row_len , 3]\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 4] = (grw / lm_rtb.iloc[row_len , 3]) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    lm_rtb.iloc[row_len , 5] = (lm_rtb.iloc[a , 5] + lm_rtb.iloc[b , 5] + lm_rtb.iloc[c , 5] + lm_rtb.iloc[d , 5]).round()\n",
    "    lm_rtb.iloc[row_len , 6] = (lm_rtb.iloc[a , 6] + lm_rtb.iloc[b , 6] + lm_rtb.iloc[c , 6] + lm_rtb.iloc[d , 6]).round()\n",
    "    grw = lm_rtb.iloc[row_len , 5] - lm_rtb.iloc[row_len , 6]\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 7] = (grw / lm_rtb.iloc[row_len , 6]) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 7] = \"NaN\"\n",
    "    # AUR\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 8] = (lm_rtb.iloc[row_len , 5] / lm_rtb.iloc[row_len , 2]).round()\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 8] = \"NaN\"\n",
    "\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 9] = (lm_rtb.iloc[row_len , 6] / lm_rtb.iloc[row_len , 3]).round()\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 9] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len , 10] = (lm_rtb.iloc[row_len , 8] - lm_rtb.iloc[row_len , 9]).round()\n",
    "    # COST\n",
    "    lm_rtb.iloc[row_len , 11] = (lm_rtb.iloc[a , 11] + lm_rtb.iloc[b , 11] + lm_rtb.iloc[c , 11] + lm_rtb.iloc[d , 11]).round()\n",
    "    lm_rtb.iloc[row_len , 12] = (lm_rtb.iloc[a , 12] + lm_rtb.iloc[b , 12] + lm_rtb.iloc[c , 12] + lm_rtb.iloc[d , 12]).round()\n",
    "    grw = lm_rtb.iloc[row_len , 11] - lm_rtb.iloc[row_len , 12]\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 13] = (grw / lm_rtb.iloc[row_len , 12]) * 100\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 13] = \"NaN\"\n",
    "    # DISCOUNT\n",
    "    grw = lm_rtb.iloc[a , 14] + lm_rtb.iloc[b , 14] + lm_rtb.iloc[c , 14] + lm_rtb.iloc[d , 14]\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 14] = (grw / 4).round()\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 14] = \"NaN\"\n",
    "\n",
    "    grw = lm_rtb.iloc[a , 15] + lm_rtb.iloc[b , 15] + lm_rtb.iloc[c , 15] + lm_rtb.iloc[d , 15]\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 15] = (grw / 4).round()\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 15] = \"NaN\"\n",
    "    lm_rtb.iloc[row_len , 16] = lm_rtb.iloc[row_len , 14] - lm_rtb.iloc[row_len , 15]\n",
    "    # GROSS MARGIN\n",
    "    grw = lm_rtb.iloc[a , 17] + lm_rtb.iloc[b , 17] + lm_rtb.iloc[c , 17] + lm_rtb.iloc[d , 17]\n",
    "    try:\n",
    "        lm_rtb.iloc[row_len , 17] = (grw / 4).round()\n",
    "    except:\n",
    "        lm_rtb.iloc[row_len , 17] = \"NaN\"\n",
    "\n",
    "    lm_rtb[\"Qty_Grw\"] = lm_rtb[\"Qty_Grw\"] / 100\n",
    "    lm_rtb[\"GSales_Grw\"] = lm_rtb[\"GSales_Grw\"] / 100\n",
    "    lm_rtb[\"Cost_Grw\"] = lm_rtb[\"Cost_Grw\"] / 100\n",
    "    lm_rtb[\"Disc_TM\"] = lm_rtb[\"Disc_TM\"] / 100\n",
    "    lm_rtb[\"Disc_LM\"] = lm_rtb[\"Disc_LM\"] / 100\n",
    "    lm_rtb[\"Disc_Diff\"] = lm_rtb[\"Disc_Diff\"] / 100\n",
    "    lm_rtb[\"GMargin\"] = lm_rtb[\"GMargin\"] / 100\n",
    "    \n",
    "    lm_rtb = lm_rtb.replace([np.inf, -np.inf], 0) # Replace NaN and Inf\n",
    "    lm_rtb = lm_rtb.replace([np.nan], 0) # Replace NaN and Inf\n",
    "    return lm_rtb\n",
    "\n",
    "\n",
    "    \n",
    "\n"
   ]
  },
