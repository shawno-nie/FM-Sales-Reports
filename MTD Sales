    "    except:\n",
    "        rtb.iloc[row_len, 4] = \"NaN\"\n",
    "    # GROSS SALES\n",
    "    rtb.iloc[row_len, 5] = CPT[\"Gross_Sales\"].sum()\n",
    "    rtb.iloc[row_len, 6] = CPT[\"GSales_LY\"].sum()\n",
    "    grw = CPT[\"Gross_Sales\"].sum() - CPT[\"GSales_LY\"].sum()\n",
    "    try:\n",
    "        rtb.iloc[row_len, 7] = (grw / CPT[\"GSales_LY\"].sum()) * 100\n",
    "    except:\n",
    "        rtb.iloc[row_len, 7] = \"NaN\"\n",
